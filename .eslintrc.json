{
    "env": {
        "browser": true,
        "es6": true,
        "jsx": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "comma-dangle": [
          1, "always-multiline"
        ],
        "no-unused-vars": [
          1, {"vars": "local", "args": "none"}
        ],
        "no-console": "off"
    }
}
